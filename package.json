{
  "name": "travel-plan",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@material-ui/core": "^3.9.3",
    "@material-ui/icons": "^3.0.2",
    "firebase": "^5.11.1",
    "lodash": "^4.17.21",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-redux": "^7.0.3",
    "react-scripts": "2.1.8",
    "redux": "^4.0.1",
    "redux-thunk": "^2.3.0",
    "styled-components": "^4.2.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "test:CI": "CI=true react-scripts test --color --reporters=default --reporters=jest-junit --coverage --coverageDirectory=output/coverage/jest",
    "eject": "react-scripts eject"
  },
  "jest": {
    "collectCoverageFrom": [
